{
    "name": "seyls/accounting",
    "description": "IFRS compliant accounting for laravel. see ekmungai/eloquent-ifrs",
    "keywords": [
        "seyls",
        "accounting"
    ],
    "homepage": "https://github.com/seyls/accounting",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Hicker",
            "email": "hickr@me.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Seyls\\Accounting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seyls\\Accounting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seyls\\Accounting\\AccountingServiceProvider"
            ],
            "aliases": {
                "Accounting": "Seyls\\Accounting\\AccountingFacade"
            }
        }
    }
}
